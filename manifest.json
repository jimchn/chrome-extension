{
  "manifest_version": 2,
  "name": "Tuturu Alarm Clock",
  "version": "0.1",
  "description": "Pair Programming Timer",
  "browser_action": {
    "default_icon": "tuturu.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
}
